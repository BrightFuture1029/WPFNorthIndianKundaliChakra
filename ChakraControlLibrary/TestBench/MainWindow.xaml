<Window x:Class="TestBench.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestBench" xmlns:chakracontrollibrary="clr-namespace:ChakraControlLibrary;assembly=ChakraControlLibrary"
        mc:Ignorable="d" x:Name="parent" DataContext="{DynamicResource vm1}"
        Title="Test Bench" d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel>
            <Button Content="Calculate" Command="{Binding CalculateCmd}"/>
        </StackPanel>
        <chakracontrollibrary:NorthIndianKundaliChart Grid.Row="1" />
    </Grid>
</Window>
